  <body>
    <section id="header">
      <div class="header-logo">
        <img src="../static/img/logo.png" alt="" />
      </div>
      <div class="header-text"><b>chat</b></div>
      <ul id="navbar">
        <li><a class="active" href="#hero">Home</a></li>
      </ul>
    </section>

    <section id="hero">
      <div class="hero-text">
        <h3>Enjoy group chat with friends!</h3>
        <h2>That's <span class="hero">chat</span> for you</h2>
        <a href="#chat">
          <button class="btn1">Join Chat</button>
        </a>
      </div>
      <div class="hero-img">
        <img src="../static/img/hero2.png" alt="" />
      </div>
    </section>

    

    <section id="chat">
      <div class="container">
        <header>
          <div class="header-logo">
            <img src="../static/img/logo.png" alt="" />
          </div>
          <div class="header-text"><b>chat</b></div>
        </header>
        <section class="section-container">
          <form action="/chatroom" style="width: 400px; margin-top: 25px">
            <div class="form-control">
              <label for="username">Username</label>
              <input
                type="text"
                name="username"
                id="username"
                placeholder="  Enter username..."
                required
                autocomplete="off"
              />
            </div>
            <div class="form-control">
              <label for="room">Room</label>
              <select name="room" id="room" required>
                <option value="">Select a room</option>
                <option value="General">General</option>
                <option value="Games">Games</option>
                <option value="Movies">Movies</option>
                <option value="Music">Music</option>
                <option value="Code">Code</option>
                <option value="DSA">DSA</option>
              </select>
            </div>
            <button type="submit" class="btn">
              <a href="../views/chat.ejs"> Join Chat</a>
            </button>
          </form>
        </section>
        <img class="send" src="../static/img/send.svg" alt="" />
      </div>
    </section>

    <section id="footer">
      <div class="footer-text">Made with ‚ù§ by : Developers</div>
    </section>
